<div class="container mt-3">
    <div class="row">
        <div class="col-12">
            <h1>Who let the Humans out!</h1>
        </div>
    </div>

    <div class="row">
        <% @humans.each do |human| %>
            <h4>Name: <%= human.name %> </h4>
            <h4>Description: <%= human.description %> </h4>
            <h4>Address: <%= human.address %> </h4>
            <h4>Rating: <%= human.rate %> </h4>
        <% end %>
    </div>
</div>
